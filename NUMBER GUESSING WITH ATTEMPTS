
import random
number = random.randint(1, 20)
attempts = 5
while attempts > 0:
    guess = int(input("Guess a number (1-20): "))
    if guess == number:
        print("Correct! You win!")
        break
    else:
        print("Wrong! Attempts left:", attempts - 1)
    attempts -= 1
if attempts == 0:
    print("Out of attempts! The number was", number)
